{
  "name": "docker Iperf",
  "vendor": "fokus",
  "version": "0.1-ALPHA",
  "vld": [{
    "name": "new-network"
  }],
  "vnfd": [{
    "name": "server",
    "vendor": "TUB",
    "version": "0.2",
    "lifecycle_event": [

    ],
    "configurations": {
      "configurationParameters": [],
      "name": "server-configuration"
    },
    "virtual_link": [{
      "name": "new-network"
    }],
    "vdu": [{
      "vm_image": [
        "openbaton/iperfserver:latest"
      ],
      "scale_in_out": 2,
      "vnfc": [{
        "connection_point": [{
          "virtual_link_reference": "new-network"
        }]
      }]
    }],
    "deployment_flavour": [{
      "flavour_key": "m1.small"
    }],
    "type": "server",
    "endpoint": "docker"
  }, {
    "name": "client",
    "vendor": "TUB",
    "version": "0.2",
    "lifecycle_event": [

    ],
    "configurations": {
      "configurationParameters": [],
      "name": "server-configuration"
    },
    "virtual_link": [{
      "name": "new-network"
    }],
    "vdu": [{
      "vm_image": [
        "openbaton/iperfclient:latest"
      ],
      "scale_in_out": 2,
      "vnfc": [{
        "connection_point": [{
          "virtual_link_reference": "new-network"
        }]
      }]
    }],
    "deployment_flavour": [{
      "flavour_key": "m1.small"
    }],
    "type": "client",
    "endpoint": "docker"
  }],
  "vnf_dependency": [{
    "source": {
      "name": "server"
    },
    "target": {
      "name": "client"
    },
    "parameters": [
      "hostname"
    ]
  }]
}
